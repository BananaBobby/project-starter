{
  "extends": "tslint-config-airbnb",
  "rules": {
    "import-name": { "severity": "off" },
    "max-line-length": [true, 120],
    "variable-name": false,
    "prefer-array-literal": false,
    "object-shorthand-properties-first": false
  },
  "linterOptions": {
    "exclude": [
      "src/**/*.pcss",
      "src/**/*.svg"
    ]
  }
}
