{
  "parser": "babel-eslint",
  "extends": [
    "airbnb/base"
  ],
  "globals": {
    "__DEV__": true
  },
  "env": {
    "node": true
  },
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "space-before-function-paren": 0,
    "max-len": ["error", 120],
    "class-methods-use-this" : 0,
    "object-curly-newline": 0,
    "import/prefer-default-export": 0
  }
}
